<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


    <TextView
            android:text="childNameTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteY="71dp"
            tools:layout_editor_absoluteX="35dp"
            android:id="@+id/child_name"/>
    <TextView
            android:text="childSurnameTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteY="118dp"
            tools:layout_editor_absoluteX="62dp"
            android:id="@+id/child_surname"/>
    <TextView
            android:text="childAgeTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteY="178dp"
            tools:layout_editor_absoluteX="35dp"
            android:id="@+id/child_age"/>
    <TextView
            android:text="childActivitiesTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteY="259dp"
            tools:layout_editor_absoluteX="48dp"
            android:id="@+id/child_activities"/>
    <TextView
            android:text="childInterestTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteY="225dp"
            tools:layout_editor_absoluteX="48dp"
            android:id="@+id/child_interest"/>

</LinearLayout>
